{
  "name": "vscode-h5web",
  "displayName": "H5Web",
  "publisher": "ESRF",
  "description": "Visualize HDF5 files in VSCode",
  "version": "0.0.1",
  "author": "European Synchrotron Radiation Facility",
  "license": "MIT",
  "homepage": "https://h5web.panosc.eu/",
  "repository": {
    "type": "git",
    "url": "https://github.com/silx-kit/vscode-h5web.git"
  },
  "bugs": {
    "url": "https://github.com/silx-kit/vscode-h5web/issues",
    "email": "h5web@esrf.fr"
  },
  "icon": "./assets/favicon192.png",
  "galleryBanner": {
    "color": "#333333",
    "theme": "light"
  },
  "categories": [
    "Visualization"
  ],
  "engines": {
    "node": "16.x",
    "pnpm": "7.x",
    "vscode": ">=1.34.0"
  },
  "activationEvents": [
    "onCustomEditor:h5web.viewer"
  ],
  "contributes": {
    "customEditors": [
      {
        "viewType": "h5web.viewer",
        "displayName": "H5Web",
        "selector": [
          {
            "filenamePattern": "*.{h5,hdf,hdf5,nx,nxs,nx5,nexus,cxi}"
          }
        ],
        "priority": "default"
      }
    ]
  },
  "main": "./out/extension.js",
  "scripts": {
    "prewatch": "pnpm build:app",
    "watch": "tsc --project ./src --watch",
    "build": "pnpm build:app && pnpm build:extension",
    "build:app": "vite build",
    "build:extension": "tsc --project ./src",
    "prettier": "prettier . --cache --check",
    "vscode:prepublish": "pnpm build"
  },
  "dependencies": {
    "@h5web/app": "6.1.0",
    "@h5web/h5wasm": "6.1.0",
    "@react-hookz/web": "15.0.1",
    "axios": "0.27.2",
    "normalize.css": "8.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "suspend-react": "0.0.8"
  },
  "devDependencies": {
    "@types/node": "16.11.7",
    "@types/react": "17.0.33",
    "@types/react-dom": "17.0.10",
    "@types/vscode": "1.68.1",
    "@vitejs/plugin-react": "1.3.2",
    "prettier": "2.7.1",
    "typescript": "4.7.3",
    "vite": "2.9.13"
  }
}
